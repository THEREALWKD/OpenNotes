{
  "manifest_version": 3,
  "name": "YouTube OpenNotes",
  "version": "1.14",
  "description": "A Open-Source Vertical Notepad for YouTube videos",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
  "16": "icon16.png",
  "32": "icon32.png",
  "48": "icon48.png",
  "128": "icon128.png"
}
}
